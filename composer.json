{
  "name": "cashid/cashid-library",
  "type": "library",
  "description": "CashID library for PHP",
  "keywords": ["bitcoin", "authentication"],
  "require": {
    "php": ">=7.0.0",
    "bitcoin-php/bitcoin-ecdsa": ">=1.3",
    "submtd/cashaddr-converter": ">=1.0.1"
  },
  "autoload": {
    "psr-4": {"CashID\\": "src/"}
  },
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "php-coveralls/php-coveralls": "^2.0",
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload-dev": {
    "psr-4": {
      "CashID\\Tests\\": "tests/",
      "CashID\\": "tests/Overriders/"
    }
  },
  "authors": [
    {
      "name": "Jonathan Silverblood",
      "role": "Lead developer"
    },
    {
      "name": "Kevin Nowaczyk",
      "role": "Contributor"
    }
  ],
  "license": "MIT"
}
