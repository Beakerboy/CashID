{
  "name": "cashid/cashid-library",
  "type": "library",
  "description": "CashID library for PHP",
  "keywords": ["bitcoin", "authentication"],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": { "CashID\\": "src/" }
  },
  "require-dev": {
      "phpunit/phpunit": "6.*",
      "php-coveralls/php-coveralls": "^2.0",
      "squizlabs/php_codesniffer": "2.*"
  },
  "autoload-dev": {
    "psr-4": { "CashID\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Jonathan Silverblood",
      "role": "Lead developer"
    },
    {
      "name": "Kevin Nowaczyk",
      "role": "Contributor"
    }
  ],
  "license": "MIT"
}
