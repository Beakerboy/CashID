{
  "name": "beakerboy/cashid-library",
  "type": "library",
  "description": "CashID library for PHP",
  "keywords": ["bitcoin", "authentication"],
  "require": {
    "php": ">=7.1.0",
    "bitcoin-php/bitcoin-ecdsa": ">=1.3",
    "submtd/cashaddr-converter": ">=1.0.1"
  },
  "autoload": {
    "psr-4": {"CashID\\": "src/"}
  },
  "require-dev": {
    "phpstan/phpstan": ">0.11.0",
    "phpunit/phpunit": "7.*",
    "php-coveralls/php-coveralls": "^2.0",
    "squizlabs/php_codesniffer": "3.*",
    "suin/phpcs-psr4-sniff": "2.*",
    "php-mock/php-mock-phpunit": ">=2.4.0"
  },
  "autoload-dev": {
    "psr-4": {
      "CashID\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Jonathan Silverblood",
      "role": "Lead developer"
    },
    {
      "name": "Kevin Nowaczyk",
      "role": "Contributor"
    }
  ],
  "license": "MIT"
}
